
    
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BeleanIoan_DanielLab7.ListEntryPage">
    <StackLayout>
        <ListView x:Name="listView" ItemSelected="OnListViewItemSelected">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding Name}" Detail="{Binding Date}" />
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
        <Button Text="Add Shopping List" Clicked="OnShopListAddedClicked"  />
    </StackLayout>
</ContentPage>

